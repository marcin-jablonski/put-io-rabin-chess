{
  "name": "rabin-chess",
  "version": "1.0.0",
  "description": "Rabin Chess - chess app built in react",
  "author": "Marek Rydlewski Marcin Jabloński Konrad Matusiak Michał Panowicz",
  "license": "MIT",
  "config": {
    "port": 9000
  },
  "scripts": {
    "clean": "rimraf build/ *.log",
    "build": "npm run clean && cross-env NODE_ENV=production && webpack --config webpack/build.config.js",
    "dev": "npm run clean && webpack-dev-server --config webpack/dev.config.js",
    "test": "jest",
    "flow": "flow check --color always --show-all-errors",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint src/scripts/**/*",
    "lint:sass": "sass-lint -v src/styles/**/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcin-jablonski/rabin-chess.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.0",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^9.0.0",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "cross-env": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.5.0",
    "eslint-plugin-flowtype": "^2.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.0",
    "flow-bin": "^0.22.0",
    "html-webpack-plugin": "^2.15.0",
    "jest-cli": "^0.9.0",
    "json-loader": "^0.5.0",
    "node-sass": "^3.4.0",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.5.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^2.0.1"
  }
}
